{
  "name": "MINUTE",
  "category": "Date/Time",
  "shortDescription": "Extract minute from timestamp",
  "syntax": [
    "MINUTE(timestamp)"
  ],
  "description": "Extracts the minute component (0-59) from a timestamp.",
  "parameters": [
    {
      "name": "timestamp",
      "type": "TIMESTAMP",
      "description": "Timestamp to extract minute from",
      "optional": false,
      "defaultValue": null
    }
  ],
  "returnType": "INT",
  "examples": [
    {
      "title": "Get minute",
      "description": "Extract minute from timestamp",
      "sql": "SELECT MINUTE(created_at) as minute FROM logs"
    }
  ],
  "notes": [
    "Returns 0-59"
  ],
  "seeAlso": ["HOUR", "SECOND"],
  "aliases": []
}
