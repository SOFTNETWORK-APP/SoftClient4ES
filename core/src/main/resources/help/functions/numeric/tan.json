{
  "name": "TAN",
  "category": "Numeric",
  "shortDescription": "Calculate tangent",
  "syntax": [
    "TAN(angle)"
  ],
  "description": "Returns the tangent of an angle in radians.",
  "parameters": [
    {
      "name": "angle",
      "type": "DOUBLE",
      "description": "Angle in radians",
      "optional": false,
      "defaultValue": null
    }
  ],
  "returnType": "DOUBLE",
  "examples": [
    {
      "title": "Calculate tangent",
      "description": "Tan of 45 degrees",
      "sql": "SELECT TAN(RADIANS(45)) as tan_45"
    }
  ],
  "notes": [
    "Undefined at 90°, 270°, etc."
  ],
  "seeAlso": ["SIN", "COS", "ATAN"],
  "aliases": []
}
